{
  "root": "./docs",
  "plugins": ["expandable-chapters", "anchors", "splitter", "forkmegithub", "etoc","github-issue-feedback"],
  "pluginsConfig": {
    "lunr": {
      "maxIndexSize": 1000000
    },
    "forkmegithub": {
      "url": "https://github.ibm.com/WH-GovHHS/GovHHS-Analytics-Playbook",
